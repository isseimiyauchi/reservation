<div class="container-fuild room-posts">
    <h4 class="pb-4">登録済ルーム一覧</h4>
     
        <table>
          <thead>
            <tr align="center">
              <th>ルーム画像</th>
              <th>ルーム名</th>
              <th>ルーム紹介</th>
              <th>料金</th>
              <th>作成日</th>
            </tr>
          </thead>
     
          <tbody>
            <% @rooms.each do |room| %>
              <tr>
                <td><%= image_tag room.image.variant(resize:'200x200') %></td>
                <td><%= room.room_name %></td>
                <td><%= room.room_introduction %></td>
                <td><%= room.price %></td>
                <td><%= room.created_at %></td>
              </tr>
            <% end %>
          </tbody>
        </table>
</div>